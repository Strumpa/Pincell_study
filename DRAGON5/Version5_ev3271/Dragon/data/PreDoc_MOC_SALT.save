#!/bin/sh
#
if [ $# = 0 ]
   then
   echo "usage: PreDoc_MOC_SALT.save directory" 1>&2
   exit 1
fi
echo "PreDoc_MOC_SALT.save"
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi
ls -l
mv PIN_CELL.ps $1/"$MACH"
echo "PreDoc_MOC_SALT.save completed"

